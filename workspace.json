{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "properties": {
        "workspace": {
            "type": "object",
            "properties": {
                "workspaceId": {
                    "type": "string"
                },
                "title": {
                    "type": "string"
                },
                "metadata": {
                    "oneOf": [
                        {
                            "type": "object",
                            "properties": {
                                "APIVersion": {
                                    "type": "string"
                                }
                            },
                            "required": [
                                "APIVersion"
                            ],
                            "additionalProperties": false
                        },
                        {
                            "type": "null"
                        }
                    ]
                },
                "snapshot": {
                    "type": "object",
                    "properties": {
                        "windows": {
                            "type": "array",
                            "items": {
                                "type": "object",
                                "additionalProperties": true
                            }
                        }
                    },
                    "required": [
                        "windows"
                    ]
                }
            },
            "required": [
                "workspaceId",
                "title",
                "snapshot"
            ]
        }
    },
    "required": [
        "workspace"
    ]
}