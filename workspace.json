{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "definitions": {
        "workspaceMetadata": {
            "type": "object",
            "properties": {
                "APIVersion": {
                    "type": "string"
                }
            },
            "required": [
                "APIVersion"
            ],
            "additionalProperties": false
        },
        "browserSnapshotWindow": {
            "type": "object",
            "additionalProperties": true
        },
        "browserSnapshot": {
            "type": "object",
            "properties": {
                "windows": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/browserSnapshotWindow"
                    }
                }
            },
            "required": [
                "windows"
            ]
        },
        "workspace": {
            "type": "object",
            "properties": {
                "workspaceId": {
                    "type": "string"
                },
                "title": {
                    "type": "string"
                },
                "metadata": {
                    "oneOf": [
                        {
                            "$ref": "#/definitions/workspaceMetadata"
                        },
                        {
                            "type": "null"
                        }
                    ]
                },
                "snapshot": {
                    "$ref": "#/definitions/browserSnapshot"
                }
            },
            "required": [
                "workspaceId",
                "title",
                "snapshot"
            ]
        }
    },
    "type": "object",
    "properties": {
        "workspace": {
            "$ref": "#/definitions/workspace"
        }
    },
    "required": [
        "workspace"
    ]
}